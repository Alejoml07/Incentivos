{

  "version": "2.0",
  "extensions": {
    "http": {
      "routePrefix": "api",
      "maxOutstandingRequests": 20,
      "maxConcurrentRequests": 10,
      "dynamicThrottlesEnabled": false,
      "customHandler": {
        "description": {
          "default": {
            "route": "{function}/{id?}",
            "routes": [
              {
                "route": "csharp/products/{category:alpha}/{id:int?}",
                "allowedMethods": [ "get", "post" ]
              }
            ]
          }
        }
      }
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  }

}
